@model IEnumerable<GroupProject.Models.Comment>

@{
    ViewBag.Title = "Contact Us";
}

<h2>Contact Us</h2>

<address>
    Pet Rock Lane<br />
    Lincoln, NE<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">petrock@rocks.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">petrockmarketing@rocks.com</a>
</address>
<br />
<br />
<br />






<p>
    @Html.ActionLink("Click here to send us a comment or question", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Note)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Note)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>


